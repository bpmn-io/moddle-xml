{
  "name": "moddle-xml",
  "version": "10.1.0",
  "description": "XML import/export for documents described with moddle",
  "scripts": {
    "all": "run-s lint test test:distro",
    "build": "rollup -c",
    "dev": "npm test -- --watch",
    "lint": "eslint . --ext cjs,js",
    "test": "mocha --reporter=spec --recursive test/spec",
    "test:distro": "mocha --reporter=spec --recursive test/distro",
    "pretest": "run-s build",
    "prepare": "run-s build"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/moddle-xml"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "umd:main": "dist/moddle-xml.umd.cjs",
  "keywords": [
    "moddle",
    "model",
    "meta-model",
    "xml",
    "xsd",
    "import",
    "export"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "chai": "^4.3.6",
    "eslint": "^8.34.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "mocha": "^10.0.0",
    "moddle": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.17.2"
  },
  "dependencies": {
    "min-dash": "^4.0.0",
    "saxen": "^10.0.0"
  },
  "peerDependencies": {
    "moddle": ">= 6.2.0"
  }
}
